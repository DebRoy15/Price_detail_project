package exam;

import java.util.*;

public class Date_06_01_2020_1 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the length of stick");
		int n = sc.nextInt();
		System.out.println("Enter the sticks");
		int a[]= new int[n];
		int sum=0;
		for(int i=0;i<=n-1;i++)
		{
			 a[i]= sc.nextInt();
		}System.out.println();
		Arrays.sort(a);
		int s=a.length;
		for(int i=0;i<a.length;i++)
		{
			if(i==i-1)
			{
				System.out.println(" ");
			}
			else {
			System.out.println(s);
			}
			s=a.length- a[i];
		}

	}

}
