package exam;

public class StarPatternProg1 {

	public static void main(String[] args) {
		System.out.println("enter the cource");
		int space= 4;
		for(int i=0;i<5;i++) {
			System.out.print(" ");
			for(int j=0;j<=i;j++) {
				System.out.print(" "+"*");
			}
		System.out.println();
		}
	space--;
	}
}
