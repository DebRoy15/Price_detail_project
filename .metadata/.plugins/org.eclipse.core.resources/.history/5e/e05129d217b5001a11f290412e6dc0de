package test;

import java.util.Scanner;

public class Test {

	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		
		String number = scan.nextLine();
	
	         for(int i = number.length()-1; i>=0; i--) {
			   System.out.print(number.charAt(i));
			   }
	         System.out.println();
	         
	         scan.close();

	}

}
