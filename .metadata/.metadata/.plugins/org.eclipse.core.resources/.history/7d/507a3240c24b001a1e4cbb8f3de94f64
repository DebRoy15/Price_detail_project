package java_Basics;

public class NegativeFirst {

	public static void main(String[] args) {
		int arr[]= {10,20,-3,30,-4};
		int temp=0,j=0;
		for(int i= 0;i<=arr.length-1;i++)
		{
				if(arr[i]<0)
				{
					temp=arr[j];
					arr[j]=arr[i];
					arr[i]=temp;
				}
			
		}
		for(int i=0;i<=arr.length-1;i++)
		{
			System.out.print(arr[i]+" ");
		}
		System.out.println();

	}

}
