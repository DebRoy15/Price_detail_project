package helloMRDev;

import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
      int a[]= {1,2,3,4,5};
      for(int i=a.length-1;i<=0;i--) {
    	  System.out.println(a[i]);
      }
      for(int i=0;i<=a.length-1;i++) {
    	  System.out.println(a[i]);
      }
      

    }
}
