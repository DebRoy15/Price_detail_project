package exam;

import java.util.LinkedList;

public class Collection {

	public static void main(String[] args) {
		LinkedList ll = new LinkedList();
		ll.add(10);
		ll.add(20);
		ll.add(30);
		ll.add(true);
		ll.add("Hello");
		ll.add('$');
		System.out.println(ll);
		System.out.println(".......................");
		System.out.println(ll.isEmpty());

	}

}
