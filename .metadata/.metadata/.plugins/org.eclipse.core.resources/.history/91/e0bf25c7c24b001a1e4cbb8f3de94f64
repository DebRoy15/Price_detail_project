package java_Basics;

public class NegativeFirst {

	public static void main(String[] args) {
		int arr[]= {2,-3,-4,-5,-6,7};
		int temp=0,j=0;
		for(int i= 0;i<=arr.length-1;i++)
		{
				if(arr[i]<0)
				{
					swap(arr[i],arr[j]);
					j++;
				}
			
		}
		for(int i=0;i<=arr.length-1;i++)
		{
			System.out.print(arr[i]+" ");
		}
		System.out.println();

	}
    static void swap(int a, int b)
    {
    	int temp=0;
    	temp= a;
		a = b;
		 b =temp;
    }
}
