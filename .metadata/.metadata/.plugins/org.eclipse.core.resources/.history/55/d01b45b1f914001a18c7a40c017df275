	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		System.out.println("Enter some value");
		int array = scan.nextInt();
		
		int a[] = new int[array];
		     Arrays.sort(a);
		      
		     System.out.println(Arrays.toString(a));

	}

}
