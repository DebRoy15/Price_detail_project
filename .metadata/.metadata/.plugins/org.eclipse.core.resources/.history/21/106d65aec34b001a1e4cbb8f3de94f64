package java_Basics;

public class NegativeFirst {

	public static void main(String[] args) {
		int arr[]= {-3,-4,-5,-6,7,-8};
		int temp=0,j=0;
		for(int i= 0;i<=arr.length-1;i++)
		{
				if(arr[i]<0 && i != j)
				{
					temp=arr[j];
					arr[j]=arr[i];
					arr[i]=temp;
				}
				j++;
		}
		for(int i=0;i<=arr.length-1;i++)
		{
			System.out.print(arr[i]+" ");
		}
		System.out.println();

	}

}
