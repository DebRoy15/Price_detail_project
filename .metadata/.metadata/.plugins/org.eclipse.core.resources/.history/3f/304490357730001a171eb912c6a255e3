package exam;

import java.util.Scanner;

public class Date_06_01_2020_1 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the length f stick");
		int n = sc.nextInt();
		System.out.println("Enter the sticks");
		int a[]= new int[n];
		int sum=0;
		for(int i=0;i<=n-1;i++)
		{
			 a[i]= sc.nextInt();
			 System.out.println(a[i]);
		}
		

	}

}
