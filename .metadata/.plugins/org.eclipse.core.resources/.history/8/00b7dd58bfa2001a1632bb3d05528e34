package helloMRDev;

import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
      int a[]= {1,2,3,4,5};
     int temp=0;
      for(int i=a.length;i>0;i--) {
    	  temp=a[i];
    	  a[i]=a[i-1];
    	  a[i-1]=temp;
      }
      System.out.println(Arrays.toString(a));
      

    }
}
