package application;

import java.util.ArrayList;

public class Application {

	public static void main(String[] args) {
		ArrayList<String> animal = new ArrayList<String>();
		animal.add("Lion");
		animal.add("Cat");
		animal.add("Dog");
		animal.add("Bird");
		
		for(int i=0;i<animal.size();i++)
		{
			System.out.println(animal.get(i));
		}

	}

}
