package supai;
public class Prob1 {

	public static void main(String[] args) {
		int ar[]= {10,20,20,10,30,10,50,10,20};
	  int result=0;
      for(int i=0;i<ar.length;i++)
      {
              if(ar[i]==ar[i+1])
              {
                    result++;
                    i=i+1;
          }
      }
	System.out.println(result);
	
	}
	
   
    	 
     }
